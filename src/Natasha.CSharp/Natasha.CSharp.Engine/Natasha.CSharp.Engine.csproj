<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Description>Natasha C#编译引擎</Description>
		<PackageId>DotNetCore.Natasha.CSharp.Engine</PackageId>
		<PackageReleaseNotes>升级</PackageReleaseNotes>
		<Version>3.6.0</Version>
		<PackageTags>Roslyn;IL;Script;Dynamic;Natasha;</PackageTags>
		<FileVersion>3.6.0</FileVersion>
		<AssemblyVersion>3.6.0</AssemblyVersion>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="DotNetCore.Compile.Environment" Version="3.0.0" />
		<PackageReference Include="Microsoft.CodeAnalysis.CSharp.Workspaces" Version="4.0.1" />
	</ItemGroup>

	<ItemGroup Condition="'$(TargetFramework)' == 'netcoreapp3.0' OR '$(TargetFramework)' == 'netcoreapp3.1'">
		<PackageReference Include="Microsoft.Extensions.DependencyModel" Version="6.0.0" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net5.0' OR '$(TargetFramework)' == 'net5.1'">
		<PackageReference Include="Microsoft.Extensions.DependencyModel" Version="6.0.0" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net6.0'">
		<PackageReference Include="Microsoft.Extensions.DependencyModel" Version="6.0.0" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\Natasha.Framework\Natasha.Framework.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Update="DotNetCore.SourceLink.Environment" Version="3.1.0" />
	</ItemGroup>


</Project>
